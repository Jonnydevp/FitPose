# Railway конфигурация
PORT=8000
HOST=0.0.0.0

# OpenAI API для анализа (ОБЯЗАТЕЛЬНО ЗАПОЛНИТЕ!)
OPENAI_API_KEY=your_openai_api_key_here

# Настройки приложения
